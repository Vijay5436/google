City:false})\r\n        if(temp.length) {\r\n            const id =temp[0].id;\r\n            let citiesList = csc.getCitiesOfState(id);\r\n            this.setState({\r\n                cities:citiesList\r\n            })      \r\n        }\r\n          \r\n    }\r\n\r\n    render(){\r\n\r\n    const {countries,states,cities,disableCity,disableState} = this.state;\r\n    \r\n    const countryOptions =[];\r\n    const stateOptions=[];\r\n    const cityOptions =[];\r\n\r\n    if(countries.length){\r\n        countries.map(value =>{\r\n            countryOptions.push(\r\n            <option key={value.name}>{value.name}</option>\r\n            )\r\n        })\r\n    }\r\n\r\n    if(states.length){\r\n        states.map(value =>{\r\n            stateOptions.push(\r\n            <option key={value.name}>{value.name}</option>\r\n            )\r\n        })\r\n    } \r\n    \r\n    if(cities.length){\r\n        cities.map(value =>{\r\n            cityOptions.push(\r\n            <option key={value.name}>{value.name}</option>\r\n            )\r\n        })\r\n    } \r\n    \r\n    return(\r\n        <>\r\n            <Form.Group id='form'>\r\n                <label>Country</label>\r\n                <Form.Control as=\"select\"  onChange={this.countryChange} size=\"lg\">\r\n                    <option >--- Select Country --- </option>\r\n                    {countryOptions}\r\n                </Form.Control>\r\n                <br />\r\n                <label>State</label>\r\n                    {\r\n                        stateOptions.length ?\r\n                            <Form.Control as=\"select\" disabled={this.state.disableState} onChange={this.stateChange}>\r\n                                <option >--- Select a state  --- </option>\r\n                                {stateOptions}\r\n                            </Form.Control> :\r\n                            <Form.Control as=\"select\" disabled={this.state.disableState} onChange={this.stateChange}>\r\n                                <option>--- Select a Country first --- </option> \r\n                            </Form.Control> \r\n                    }\r\n                <br />\r\n                <label>City</label>\r\n                {\r\n                    cityOptions.length ?\r\n                        <Form.Control as=\"select\" disabled={this.state.disableCity} onChange={this.stateChange}>\r\n                            <option >--- Select a City  --- </option>\r\n                            {cityOptions}\r\n                        </Form.Control> :\r\n                        <Form.Control as=\"select\" disabled={this.state.disableCity} onChange={this.stateChange}>\r\n                            <option>--- Select a State first --- </option> \r\n                        </Form.Control> \r\n                }\r\n            </Form.Group>\r\n        </>\r\n    )\r\n    }\r\n}\r\n","C:\\ReactProjects\\assesment12\\googlemap\\src\\maicomponent\\googleMap.js",["75"],"import {Map, InfoWindow, Marker, GoogleApiWrapper} from 'google-maps-react';\r\nimport React,{Component} from 'react';\r\nexport class MapContainer extends Component {\r\n  render() {\r\n    return (\r\n      <Map google={this.props.google} zoom={14}>\r\n \r\n        <Marker onClick={this.onMarkerClick}\r\n                name={'Current location'} />\r\n \r\n        {/* <InfoWindow onClose={this.onInfoWindowClose}>\r\n            <div>\r\n              <h1>{this.state.selectedPlace.name}</h1>\r\n            </div>\r\n        </InfoWindow> */}\r\n      </Map>\r\n    );\r\n  }\r\n}\r\n \r\nexport default GoogleApiWrapper({\r\n  apiKey: ('AIzaSyAVjdGEcc003pY1pYk4WMKPRc3MMC7gKpc')\r\n})(MapContainer)","C:\\ReactProjects\\assesment12\\googlemap\\src\\maicomponent\\theamContext.js",[],"C:\\ReactProjects\\assesment12\\googlemap\\src\\contextContent\\context1.js",[],"C:\\ReactProjects\\assesment12\\googlemap\\src\\contextContent\\themeButton.js",[],"C:\\ReactProjects\\assesment12\\googlemap\\src\\reducers\\bookReducer